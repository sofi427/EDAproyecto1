package proyecto1;

public class Cita {
  //atributos
  private String id;
  private Publicacion publicacion;

  //constructora
  public Cita(String pId, Publicacion pPublicacion) {
	  this.id= pId;
	  this.publicacion = pPublicacion;
  }
  
  public Publicacion getPublicacion() {
	  return this.publicacion;
  }

  @Override
  public String toString() {
      return this.id + " (cita a " + this.publicacion.getId() + "): " + this.publicacion.getTitulo();
  }
}
