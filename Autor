package proyecto1;
import java.util.Map;

public class Autor {
	private Map listaPublicaciones;
	private String id;
	private String nombre;
	
	public Autor(String pId, String pNombre) {  
		this.id= pId;
		this.nombre= pNombre;
	}
	
	public Map getListaPublicaciones() {
		return this.listaPublicaciones;
	}
	
}
