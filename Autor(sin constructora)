package proyecto1;
import java.util.Map;

public class Autor {
	private Map listaPublicaciones;
	private String id;
	private String nombre;
	
	public Autor(String pId, String pNombre) {  // esto tenemos que saber si al crear un autor lo haces desde el fichero o como si te llegasen los atributos
		this.id= pId;
		this.nombre= pNombre;
	}
	
	public Map getListaPublicaciones() {
		return this.listaPublicaciones;
	}
	
}
